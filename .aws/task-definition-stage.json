{
    "containerDefinitions": [
        {
            "name": "k2-core-stage",
            "image": "422486916789.dkr.ecr.us-east-1.amazonaws.com/k2-core-stage:latest",
            "cpu": 1024,
            "memory": 3072,
            "portMappings": [
                {
                    "name": "k2-core-stage-80-tcp",
                    "containerPort": 80,
                    "hostPort": 80,
                    "protocol": "tcp",
                    "appProtocol": "http"
                }
            ],
            "essential": true,
            "environment": [],
            "environmentFiles": [
                {
                    "value": "arn:aws:s3:::ecs-k2-core-environment-variables/.env",
                    "type": "s3"
                }
            ],
            "mountPoints": [],
            "volumesFrom": [],
            "logConfiguration": {
                "logDriver": "awslogs",
                "options": {
                    "awslogs-create-group": "true",
                    "awslogs-group": "/ecs/ecs-k2-core-stage",
                    "awslogs-region": "us-east-1",
                    "awslogs-stream-prefix": "ecs"
                }
            }
        },
        {
            "name": "k2-server-stage",
            "image": "422486916789.dkr.ecr.us-east-1.amazonaws.com/k2-server-stage:latest",
            "cpu": 1024,
            "memory": 3072,
            "portMappings": [
                {
                    "name": "k2-server-stage-3001-tcp",
                    "containerPort": 3001,
                    "hostPort": 3001,
                    "protocol": "tcp",
                    "appProtocol": "http"
                }
            ],
            "essential": true,
            "environment": [],
            "environmentFiles": [
                {
                    "value": "arn:aws:s3:::ecs-k2-server-environment-variables/.env",
                    "type": "s3"
                }
            ],
            "mountPoints": [],
            "volumesFrom": [],
            "logConfiguration": {
                "logDriver": "awslogs",
                "options": {
                    "awslogs-create-group": "true",
                    "awslogs-group": "/ecs/ecs-k2-server-stage",
                    "awslogs-region": "us-east-1",
                    "awslogs-stream-prefix": "ecs"
                }
            }
        }
    ],
    "family": "ecs-k2-stage",
    "taskRoleArn": "arn:aws:iam::422486916789:role/ecsk2TaskExecutionRole",
    "executionRoleArn": "arn:aws:iam::422486916789:role/ecsk2TaskExecutionRole",
    "networkMode": "awsvpc",
    "volumes": [],
    "placementConstraints": [],
    "requiresCompatibilities": [
        "FARGATE"
    ],
    "cpu": "2048",
    "memory": "6144",
    "runtimePlatform": {
        "cpuArchitecture": "X86_64",
        "operatingSystemFamily": "LINUX"
    },
    "tags": []
}